{{uploading-presenter/upload-summary-header
  uploadObject=uploadObject
  onToggleExpand=(action "toggleExpand")
  onCancel=(action "cancel" uploadObject)
  onMinimize=(action "minimize")}}
{{#render-later triggerRender=triggerChildrenRender}}
  {{#bs-collapse collapsed=(not isExpanded)}}
    {{uploading-presenter/upload-object-root-list
      uploadObjects=uploadObject.children
      onCancel=(action "cancel")}}
  {{/bs-collapse}}
{{/render-later}}

{{#bs-tab
  activeId=activeTab
  onChange=(action (mut activeTab))
  class="bs-tab-onedata bs-tab-modern"
  as |tab|}}
  {{#tab.pane elementId="harv-config-general-tab" title=(tt this "generalHeader")}}
    {{harvester-configuration/general harvester=harvester}}
  {{/tab.pane}}
  {{#tab.pane elementId="harv-config-gui-plugin-tab" title=(tt this "guiPluginHeader")}}
  gui
  {{/tab.pane}}
{{/bs-tab}}



{{#promise-proxy-container
  proxy=modelProxy.models
  isLoading=(and modelProxy.isLoading (not modelProxy.hasBeenLoaded))
  isLoaded=(and modelProxy.hasBeenLoaded (not modelProxy.models.isRejected))}}
  {{one-dynamic-tree
    class="border-dashed"
    definition=initialTreeState
    compareValues=modelProxy.effectivePrivilegesSnapshot
    overrideValues=modelProxy.effectivePrivilegesSnapshot
    disabledFieldsPaths=treeDisabledPaths
    valuesChanged=(action "treeValuesChanged")}}
  {{yield}}
{{/promise-proxy-container}}

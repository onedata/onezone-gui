{{one-icon icon="space"}}
{{#truncated-string class="after-icon before-status"}}
  <span class="one-label">{{item.name}}</span>
{{/truncated-string}}
{{#status-toolbar as |toolbar|}}
  {{toolbar.icon
    icon="support"
    outerText=_totalSupportSizeHuman
    hint=(tt this "totalStorageHint" size=_totalSupportSizeHuman)
  }}
  {{toolbar.icon
    icon="provider"
    outerText=_providersCount
    hint=(tt this "supportCountHint" count=_providersCount)
  }}
{{/status-toolbar}}

